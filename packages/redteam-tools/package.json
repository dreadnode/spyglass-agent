{
  "name": "@dreadnode/spyglass-agent-redteam-tools",
  "version": "0.1.0",
  "description": "Red Team Tools SDK for Spyglass Agent",
  "repository": "dreadnode/spyglass-agent",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "node ../../scripts/build_package.js",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "prerelease:version": "node ../../scripts/bind_package_version.js",
    "prerelease:deps": "node ../../scripts/bind_package_dependencies.js",
    "prepack": "npm run build",
    "prepublishOnly": "node ../../scripts/prepublish.js"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@dreadnode/spyglass-agent-core": "*",
    "@modelcontextprotocol/sdk": "^1.11.0"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "vitest": "^3.1.1"
  },
  "engines": {
    "node": ">=18"
  }
}